{
  "name": "google-chat-desktop",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.abir.googlechat",
    "productName": "Google Chat Desktop",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "index.html"
    ],
    "mac": {
      "target": "dmg",
      "icon": "icons/icon.icns"
    },
    "win": {
      "target": "nsis",
      "icon": "icons/icon.ico"
    },
    "linux": {
      "target": "AppImage",
      "icon": "icons/icon.png"
    }
  },
  "keywords": [],
  "author": "Abir Tasrif Anto",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "electron": "^35.0.2",
    "electron-builder": "^25.1.8"
  }
}
